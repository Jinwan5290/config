{
  "name": "@fbi-js/webpack-config-react",
  "version": "1.1.1",
  "description": "Webpack config for React.js project",
  "homepage": "https://github.com/fbi-js/config/tree/master/packages/webpack-config-react",
  "repository": "fbi-js/config",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsc --preserveWatchOutput -w",
    "build": "npm run clean && tsc -b .",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "format": "prettier -cw src/*.ts && npx eslint src/*.ts --fix"
  },
  "keywords": [],
  "author": "fbi",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/preset-react": "^7.12.10",
    "@fbi-js/eslint-config-react-typescript": "^1.1.0",
    "@fbi-js/tsconfig": "^1.1.1",
    "@fbi-js/webpack-config-base": "^1.2.1"
  },
  "devDependencies": {
    "webpack": "^5.13.0"
  },
  "eslintConfig": {
    "extends": ["../eslint-config-typescript"]
  }
}
